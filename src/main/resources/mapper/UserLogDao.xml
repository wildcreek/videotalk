<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mateo.videotalk.dao.UserLogDao">
    <!--为DAO接口方法提供sql语句配置-->
    <insert id="saveLog" >

        INSERT INTO
        userlog ( userID,userLog)
        VALUES
        (#{userID},#{userLog})

    </insert>
</mapper>