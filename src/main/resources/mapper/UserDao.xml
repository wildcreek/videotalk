<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mateo.videotalk.dao.UserDao">
    <!--为DAO接口方法提供sq语句配置-->
    <select id="findUserByUserAccount" >
        SELECT
          *
        FROM
          useraccount
        WHERE
          useracount = #{userAccount}

    </select>
</mapper>