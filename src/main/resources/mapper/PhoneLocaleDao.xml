<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wildcreek.videotalk.dao.PhoneLocaleDao">
    <!--为DAO接口方法提供sql语句配置-->
    <insert id="insertPhoneLocale" >

        INSERT INTO
        phonelocale ( prefix,province,city,provider,areacode,postcode)
        VALUES
        (#{prefix},#{province},#{city},#{provider},#{areacode},#{postcode})

    </insert>
</mapper>